<?xml version="1.0"?><Report><ReportQuery><Format><ReportTitle>officers</ReportTitle><ReportDescription></ReportDescription><PageSize>A4</PageSize><PageOrientation>Landscape</PageOrientation><TopMargin></TopMargin><BottomMargin></BottomMargin><RightMargin></RightMargin><LeftMargin></LeftMargin><pdfFont>dejavusans</pdfFont><pdfFontSize>9</pdfFontSize><PreExecuteCode>NONE</PreExecuteCode><formBetweenRows>solidline</formBetweenRows><gridDisplay>.DEFAULT</gridDisplay><gridSortable>.DEFAULT</gridSortable><gridSearchable>.DEFAULT</gridSearchable><gridPageable>.DEFAULT</gridPageable><gridPageSize>.DEFAULT</gridPageSize></Format><Datasource><SourceType>database</SourceType><SourceConnection><DatabaseType>mysql</DatabaseType><DatabaseName></DatabaseName><HostName></HostName><ServiceName></ServiceName><UserName></UserName><Password></Password></SourceConnection></Datasource><EntryForm><Query><TableSql></TableSql><WhereSql></WhereSql><GroupSql></GroupSql><RowSelection>all</RowSelection><SQL><QuerySql></QuerySql><SQLRaw>SELECT LOWER(p.NIC) column1, p.in_name, w.work_place, s1.work_place_id, desig.designation, g.grade, g.date_join
FROM  Personal_Details p
JOIN Service s1 ON p.ID = s1.person_id
JOIN Work_Place w ON w.ID = s1.work_place_id
JOIN General_Service g ON p.ID = g.person_id
JOIN Designation desig ON s1.designation_id = desig.ID
LEFT JOIN Province_Offices po ON s1.sub_location_id = po.ID
LEFT JOIN Zonal_Offices zn1 ON s1.sub_location_id = zn1.ID
LEFT JOIN Divisional_Offices dof ON s1.sub_location_id = dof.ID
LEFT JOIN Institute ins ON s1.sub_location_id = ins.ID
LEFT JOIN Division_List div1 ON div1.div_id IN (ins.div_id)
LEFT JOIN Zone_List z1 ON z1.zone_id IN (dof.zone_id, div1.zone_id)
LEFT JOIN District_List dl1 ON dl1.dist_id IN (z1.dist_id, zn1.dist_id)
LEFT JOIN Province_List pl1 ON pl1.province_id IN (po.province_id, dl1.province_id)
LEFT OUTER JOIN Service s2 ON p.ID = s2.person_id AND (s1.time_updated &lt; s2.time_updated OR s1.time_updated = s2.time_updated AND s1.time_updated &lt; s2.time_updated)
WHERE s2.NIC is NULL

[AND s1.work_place_id IN ({workplace})]
[AND g.grade IN ({grade})]
[AND pl1.province_id IN ({province})]

ORDER BY g.date_join</SQLRaw></SQL><QueryColumns><QueryColumn><Name>column1</Name><TableName></TableName><ColumnName>LOWER(p.NIC)</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>NIC</column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>No</Name><TableName></TableName><ColumnName></ColumnName><ColumnType></ColumnType><ColumnLength></ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF>20px</ColumnWidthPDF><ColumnWidthHTML>10px</ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>in_name</Name><TableName>p</TableName><ColumnName>in_name</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF>200px</ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>Name with initials</column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>work_place</Name><TableName>w</TableName><ColumnName>work_place</ColumnName><ColumnTyp